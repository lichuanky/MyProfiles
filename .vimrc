set encoding=utf-8
set fileencodings=utf-8,gb2312,gb18030,gbk,ucs-bom,cp936,latin1
set termencoding=utf-8
set hls
set helplang=cn
set fileformat=unix

set hlsearch

set nobackup
set noswapfile

set expandtab
set tabstop=4
set softtabstop=4
set shiftwidth=4

set history=200

syntax enable
syntax on

let g:winManagerWindowLayout='FileExplorer'
